# Agentic Godot Configuration
# Copy this file to .env and fill in your values

# Gemini API Key (get one at https://aistudio.google.com/apikey)
# If using a proxy, you can use a dummy key like "proxy-managed"
GEMINI_API_KEY=your-api-key-here

# Optional: Use a Gemini proxy URL instead of direct API access
# This allows your server to manage the API key centrally
# Example: https://gemini.zerograft.online
# GEMINI_PROXY_URL=

# Model selection - exact API model identifiers
# Gemini 3 models (recommended):
#   - gemini-3-flash-preview   (fast, good for most tasks)
#   - gemini-3-pro-preview     (complex reasoning)
GEMINI_MODEL=gemini-3-flash-preview

# Godot Bridge settings
GODOT_BRIDGE_HOST=localhost
GODOT_BRIDGE_PORT=9876

# AI Router HTTP port
AI_ROUTER_PORT=9877
